[MASTER]
ignore=urlserver_pb2.py
init-hook='import sys; sys.path.append("/usr/spark/python")'

[TYPECHECK]
extension-pkg-whitelist=gumbocy,rocksdb,mmh3

[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,fixme,locally-disabled,too-few-public-methods,no-self-use,unused-argument,wrong-import-position,superfluous-parens,similarities,file-ignored

[DESIGN]
max-locals=40
max-attributes=40
max-branches=20
max-returns=12
max-args=10

[FORMAT]
max-line-length=110